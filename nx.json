{
    "npmScope": "company-name",
    "affected": {
        "defaultBase": "develop"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*",
        ".stylelintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e", "stylelint"]
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "cli": {
        "defaultCollection": "@nrwl/workspace"
    },
    "generators": {
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "defaultProject": "startup-glossary",
    "projects": {
        "data-access-services": "libs/data-access/services",
        "features-glossary-category-module": "libs/features/glossary-category-module",
        "features-glossary-home-module": "libs/features/glossary-home-module",
        "features-glossary-home-module-vue": {
            "tags": []
        },
        "startup-glossary": "apps/startup-glossary",
        "startup-glossary-e2e": "apps/startup-glossary-e2e",
        "startup-glossary-vue": "apps/startup-glossary-vue",
        "startup-glossary-vue-e2e": "apps/startup-glossary-vue-e2e",
        "ui-modals": "libs/ui/modals",
        "util-directives": "libs/util/directives",
        "util-pipes": "libs/util/pipes"
    }
}
