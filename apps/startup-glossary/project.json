{
    "projectType": "application",
    "root": "apps/startup-glossary",
    "sourceRoot": "apps/startup-glossary/src",
    "prefix": "openforge-academy",
    "targets": {
        "build": {
            "executor": "@angular-devkit/build-angular:browser",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "preserveSymlinks": true,
                "outputPath": "dist/apps/startup-glossary",
                "index": "apps/startup-glossary/src/index.html",
                "main": "apps/startup-glossary/src/main.ts",
                "polyfills": "apps/startup-glossary/src/polyfills.ts",
                "tsConfig": "apps/startup-glossary/tsconfig.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    "apps/startup-glossary/src/assets"
                ],
                "styles": [
                    "apps/startup-glossary/src/global.scss"
                ],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "apps/startup-glossary/src/environments/environment.ts",
                            "with": "apps/startup-glossary/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all"
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@angular-devkit/build-angular:dev-server",
            "configurations": {
                "production": {
                    "browserTarget": "startup-glossary:build:production"
                },
                "development": {
                    "browserTarget": "startup-glossary:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "startup-glossary:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "apps/startup-glossary/src/**/*.ts",
                    "apps/startup-glossary/src/**/*.html"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/apps/startup-glossary"
            ],
            "options": {
                "jestConfig": "apps/startup-glossary/jest.config.js",
                "passWithNoTests": true
            }
        },
        "stylelint": {
            "executor": "nx-stylelint:lint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "apps/startup-glossary/**/*.css",
                    "apps/startup-glossary/**/*.scss"
                ]
            }
        }
    },
    "tags": []
}